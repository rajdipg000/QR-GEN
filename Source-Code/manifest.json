{
    "manifest_version": 3,
    "name": "QR GEN",
    "description": "Convert URL to QR",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "icons":
        {
           "128":"icon.png"
        }
    ,
    "permissions": [
        "tabs",
        "activeTab",
        "browsingData",
        "offscreen"
    ]
}